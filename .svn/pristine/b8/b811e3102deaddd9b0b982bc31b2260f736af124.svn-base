<!DOCTYPE html>
<html>
  <head>
    <title>函数</title>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <script type="text/javascript">
    	function fn(a,b,c,d){
//     		alert("参数值为:a="+a+",b="+b+",c="+c+",d="+d);
//     		alert("参数值为:"+arguments);
    		for(var i = 0; i < arguments.length; i++){
    			alert(arguments[i]);
    		}
    		var i = 1;
